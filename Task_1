package First_Program;

public class Task_1 {
	
	public void add()
	{
		int a=10,b=20;
		System.out.println(a);
		System.out.println(b);
		System.out.println("-----------------");
		
		
		a=a+b;
		b=a-b;
		a=a-b;
		
		System.out.println(a);
		System.out.println(b);

int c;
		c=a;
		a=b;
		b=c;
		System.out.println(a);
		System.out.println(b);
		System.out.println("-----------------");
		
	}

	public static void main(String[] args) {
		Task_1 T1= new Task_1();
		T1.add();
	}

}
